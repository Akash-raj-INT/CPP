#include <bits/stdc++.h>
using namespace std;
int main()
{
    double a,b;
    cout<<"Enter the value of number a = ";
    cin >>a;
    
    cout<<"Enter the value of number b = ";
    cin >>b;
   
    // Using + and - operators to swap two numbers without using third variable 
    a = a + b; // May cause overflow.
    b = a - b;  
    a = a - b;  
    
    cout << "After Swapping two numbers: a =" << a << ", b=" << b;
}
